    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.2",
    "listr": "^0.14.3",
    "node-sass": "^4.11.0",
    "prettier": "1.16.3",
    "sass-loader": "^7.1.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "typescript": "3.2.1",
    "vue-template-compiler": "^2.5.22",
    "vuex-module-decorators": "^0.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --ext .js --fix --ignore-path .eslintignore .",
      "git add"
    ],
    "*.vue": [
      "eslint --ext .vue --fix --ignore-path .eslintignore .",
      "git add"
    ],
    "*.ts": [
      "eslint --ext .ts --fix --ignore-path .eslintignore .",
      "git add"
    ]
  }
}
